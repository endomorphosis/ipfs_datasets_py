[run]
source = ipfs_datasets_py
omit =
    */tests/*
    */test_*.py
    *_test.py
    setup.py
    __pycache__/*
    .git/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == ['"]__main__['"]:
    if TYPE_CHECKING:
    @abstract
